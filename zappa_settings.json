{
    "dev": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "exclude": [
            "node_modules",
            "public",
            "src",
            "static",
            "binaries",
            "scripts"
        ],
        "profile_name": "{AWS_PROFILE_NAME}",
        "project_name": "{PROJECT_NAME}",
        "runtime": "python3.9",
        "s3_bucket": "{PROJECT_NAME}-dev",
        "keep_warm": false
    },
    "prod": {
        "app_function": "app.app",
        "aws_region": "us-east-1",
        "exclude": [
            "node_modules",
            "public",
            "src",
            "static",
            "binaries",
            "scripts"
        ],
        "profile_name": "{AWS_PROFILE_NAME}",
        "project_name": "{PROJECT_NAME}",
        "runtime": "python3.9",
        "s3_bucket": "{PROJECT_NAME}-prod",
        "keep_warm": false
    }
}